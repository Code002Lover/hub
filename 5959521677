if(_G.hub == nil or _G.hub == false) then return false end

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Mystikfluu/uilib/master/uilib.lua"))()
local window = library:CreateWindow("ZeroTwo Hub")
local main = window:CreateFolder("Main")
local misc = window:CreateFolder("Misc")

local lp = game.Players.LocalPlayer

function touch(part)
  firetouchinterest(part, lp.Character.HumanoidRootPart,0)
  wait()
  firetouchinterest(part, lp.Character.HumanoidRootPart,1)
end

main:Toggle("Auto Click",function(toggle)
  _G.AC = toggle
  while wait() and _G.AC do
    for i,v in pairs(game:GetService("ReplicatedStorage").Systems.Clicking.RemoteEvents:GetChildren()) do
      v:FireServer()
    end
  end
end)

main:Toggle("Auto Sell",function(toggle)
  _G.AS = toggle
  while wait() and _G.AS do
    touch(game:GetService("Workspace").Islands.Mars.SellZone.SellTouch)
  end
end)

main:Toggle("Claim Quest",function(toggle)
  _G.CQ = toggle
  while wait() and _G.CQ do
    for i,v in pairs(lp.PlayerData.Quests:GetChildren()) do
      for i2,v2 in pairs(v:GetChildren()) do
        for i3,v3 in pairs(v2:GetChildren()) do
          if(v3:IsA("Folder") and v3.Claimed.Value ~= true) then
            local args = {
                [1] = v3
            }
            game:GetService("ReplicatedStorage").Systems.Quest.ClaimQuest:FireServer(unpack(args))
          end
        end
      end
    end
  end
end)

main:Toggle("Auto Collect Drops",function(toggle)
  _G.ACD = toggle
  if(toggle) then
    lp.PlayerGui.GUI.CurrencyPopup.CurrencyPopup.Popup.Visible = false
  else
    lp.PlayerGui.GUI.CurrencyPopup.CurrencyPopup.Popup.Visible = true
  end
  while wait() and _G.ACD do
    for i,v in pairs(game:GetService("Workspace").CurrencyZones:GetChildren()) do
      if(v:FindFirstChild("Drops")) then
        for i2,v2 in pairs(v.Drops:GetChildren()) do
          DropName = v2.Name
          if(DropName == "Single") then
            DropName = "Gem"
          end
          touch(v2[DropName])
        end
      end
    end
    for i,v in pairs(game:GetService("Workspace").CurrencyNodes:GetChildren()) do
      if(v:FindFirstChild("Drops")) then
        for i2,v2 in pairs(v.Drops:GetChildren()) do
          DropName = v2.Name
          if(DropName == "Single") then
            DropName = "Gem"
          end
          if(DropName == "Chest") then
            DropName = "Hitbox"
          end
          touch(v2[DropName])
        end
      end
    end
  end
end)

main:Toggle("Auto Buy Poitions",function(toggle)
  _G.ABP = toggle
  while wait() and _G.ABP do
    local args = {
        [1] = "Potions"
    }
    game:GetService("ReplicatedStorage").Systems.Upgrade.BuyAllUpgrades:InvokeServer(unpack(args))
  end
end)

main:Toggle("Auto Buy DNA",function(toggle)
  _G.ABD = toggle
  while wait() and _G.ABD do
    local args = {
        [1] = "Dna"
    }
    game:GetService("ReplicatedStorage").Systems.Upgrade.BuyAllUpgrades:InvokeServer(unpack(args))
  end
end)

misc:Button("Infinite Jumps",function()
  lp.PlayerData.JumpsOwned.Value = 69420
end)

misc:Button("Unlock all areas",function()
  for i,v in pairs(game:GetService("Workspace").Islands:GetChildren()) do
    if(v:IsA("Part")) then
      touch(v)
    end
  end
end)

misc:Button("Collect all Chests",function()
  for i,v in pairs(game:GetService("Workspace").TimedChests:GetChildren()) do
    if(v:FindFirstChild("Glow")) then
      touch(v.Glow)
    end
  end
end)

main:Dropdown("Select Egg",game:GetService("Workspace").EggKiosks:GetChildren(),true,function(value)
  _G.Egg = value
end)

main:Toggle("Buy Eggs",function(toggle)
  _G.BE = toggle
  while wait() and _G.BE do
    local args = {
        [1] = _G.Egg.Name,
        [2] = 1
    }

    game:GetService("ReplicatedStorage").Systems.Egg.OpenEgg:InvokeServer(unpack(args))
  end
end)
