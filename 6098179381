_G.Credits = {
  "Created by Code002Lover",
  ["Discord"] = "8ytFhuj"
}

local marketplaceService = game:GetService("MarketplaceService")
function getname(id)
  return marketplaceService:GetProductInfo(id, Enum.InfoType.Asset).Name
end
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Mystikfluu/uilib/master/uilib.lua"))()
local window = library:CreateWindow(getname(game.PlaceId))
local Main = window:CreateFolder("Main")

Main:Toggle("Auto kill all Zombies",function(toggle)
_G.kill = toggle

while _G.kill and wait() do
tool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool") or game.Players.LocalPlayer.Backpack:FindFirstChildOfClass("Tool")
remote = tool.shotEvent
for i,v in pairs(game.workspace.guaiwus:GetChildren()) do
local args = {
    [1] = "takeDamage",
    [2] = {
        ["target"] = v
    }
}

remote:FireServer(unpack(args))
end
end
end)
