  "Created by Code002Lover and Ente-Sama",
  ["Discord"] = "8ytFhuj"
}
local marketplaceService = game:GetService("MarketplaceService")
function getname(id)
  return marketplaceService:GetProductInfo(id, Enum.InfoType.Asset).Name
end
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Mystikfluu/uilib/master/uilib.lua"))()
local window = library:CreateWindow(getname(game.PlaceId))
local Main = window:CreateFolder("Main")

local remotekey = "kRX6Hy.>H!qw-`ve&5XA[e!6qr[Lpu/'m_P7Vy3s"
local Farm
for i,v in pairs(game.Workspace.GeneralFarmStructures:GetChildren()) do
  if(v.Owner.Value == game.Players.LocalPlayer.Name) then
    Farm = v
  end
end
if(Farm == nil) then
  error("No farm found!")
end
function collectall()
  for i,v in pairs(Farm.Crops:GetChildren()) do
      if(v:FindFirstChild("Squares")) then
        for i2,v2 in pairs(v.Squares:GetChildren()) do
        if(v2:FindFirstChild("Big")) then
          if(v2.Big:FindFirstChild("Mask")) then
            if(v2.Big.Mask:FindFirstChild("TouchInterest")) then
              spawn(function()
                repeat
                  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v2.Big.Mask.CFrame
                  wait()
                until v2.Parent == nil or v2 == nil or v2:FindFirstChild("Big") == nil
              end)
            end
          end
        end
      end
    end
  end
end
function fireclickdetectors()
  for i,v in pairs(Farm.Crops:GetChildren()) do
    if(v:FindFirstChild("Squares")) then
      for i2,v2 in pairs(v.Squares:GetChildren()) do
        fireclickdetector(v2.ClickDetector)
      end
    end
  end
end
function plant()
  game:GetService("ReplicatedStorage").FTUEEvents.FTUEPlant:FireServer()
end
function plantcollect()
  fireclickdetectors()
  wait(0.1)
  fireclickdetectors()
  collectall()
end
function getmoney()
local args = {
    [1] = {
        ["image"] = "rbxassetid://5935772839",
        ["name"] = "Red Flags",
        ["robux"] = false,
        ["price"] = -3e17,
        ["order"] = 1,
        ["model"] = game:GetService("ReplicatedStorage").PartyItems.RedFlags
    },
    [2] = workspace.GeneralFarmStructures.FarmStructureGeneral6.Surface
}

game:GetService("ReplicatedStorage").PurchasePartyItem:FireServer(unpack(args))
end
function upgrade()
local args = {
    [1] = "Farm"
}
game:GetService("ReplicatedStorage").FarmEvents.UpgradeFarm:FireServer(unpack(args))
local args = {
    [1] = "Plant"
}
game:GetService("ReplicatedStorage").FarmEvents.UpgradeFarm:FireServer(unpack(args))
local args = {
    [1] = "Animal"
}
game:GetService("ReplicatedStorage").FarmEvents.UpgradeFarm:FireServer(unpack(args))
end
function collectlivestock()
local no = "LivestockArea-to-LivestockArea Strong Joint"
for i,v in pairs(Farm.Livestock:GetChildren()) do
for i2,v2 in pairs(v:GetChildren()) do
if(v2:IsA("MeshPart") and v2.Name ~= no) then
  spawn(function()
    repeat
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v2.CFrame
      wait()
    until v2.Parent == nil or v2 == nil
  end)
end
end
end
end



Main:Button("Plant/Collect plants",function()
plantcollect()
end)
Main:Button("Get a bunch of Money",function()
getmoney()
end)
Main:Button("Upgrade",function()
upgrade()
end)
Main:Button("Collect Livestock",function()
collectlivestock()
end)
Main:Button("Feed Livestock",function()
  local args = {
      [1] = 1,
      [2] = remotekey
  }
  game:GetService("ReplicatedStorage").AnimalEvents.Feed:FireServer(unpack(args))
end)
Main:Button("Get Robux Items",function()
  for i=0,7 do
    local args = {
        [1] = "Tractor",
        [2] = i,
        [3] = 1,
        [4] = 1,
        [5] = remotekey
    }
    game:GetService("ReplicatedStorage").Customization.PurchaseCustomization:FireServer(unpack(args))
  end
  local args = {
      [1] = "Seed",
      [2] = "Lemons",
      [3] = 100,
      [4] = "rbxassetid://4523041233",
      [5] = 1
  }

  game:GetService("ReplicatedStorage").Inventory.Buy:FireServer(unpack(args))
  local args = {
      [1] = "Animal",
      [2] = "Duck",
      [3] = 100,
      [4] = "rbxassetid://6103975571",
      [5] = 1
  }

  game:GetService("ReplicatedStorage").Inventory.Buy:FireServer(unpack(args))

  -- Script generated by SimpleSpy - credits to exx#9394

  local args = {
      [1] = "Animal",
      [2] = "Gold Chicken",
      [3] = 100,
      [4] = "rbxassetid://6103975571",
      [5] = 1
  }

  game:GetService("ReplicatedStorage").Inventory.Buy:FireServer(unpack(args))
end)

