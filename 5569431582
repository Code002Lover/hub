repeat wait() until game
repeat wait() until game:GetService("VirtualUser")
repeat wait() until game.Players.LocalPlayer


--[[

Anti AFK

]]


local VirtualUser=game:service'VirtualUser'
game:service('Players').LocalPlayer.Idled:connect(function()
VirtualUser:CaptureController()
VirtualUser:ClickButton2(Vector2.new())
end)
_G.Credits = {
  "Created by Ente_sama & Code002Lover ",
  ["Discord"] = "8ytFhuj"
}
local marketplaceService = game:GetService("MarketplaceService")
local lp = game.Players.LocalPlayer
function getname(id)
  return marketplaceService:GetProductInfo(id, Enum.InfoType.Asset).Name
end
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Mystikfluu/uilib/master/uilib.lua"))()
local window = library:CreateWindow(getname(game.PlaceId))
local main = window:CreateFolder("Main")


main:Toggle("Auto Attack",function(toggle)
  _G.AA = toggle
  while wait() and _G.AA do
game:GetService("ReplicatedStorage").Remotes.Attack:FireServer()
  end
end)

main:Toggle("Auto Collect Bubbles",function(toggle)
_G.ACB = toggle
while wait() and _G.ACB do
for i,v in pairs(game:GetService("Workspace").FoodObjects:GetChildren()) do
  if(_G.ACB and v) then
    if(lp.Character.PrimaryPart == nil) then
       lp.Character.PrimaryPart = lp.Character.HumanoidRootPart
     end
    lp.Character:SetPrimaryPartCFrame(v.CFrame)
    wait(0.5)
  end
end
end
end)

places = {
  "CaveTown",
  "ShroomTown",
  "StumpTown"
}

main:Dropdown("Teleport",places,true,function(where)
  if(lp.Character.PrimaryPart == nil) then
     lp.Character.PrimaryPart = lp.Character.HumanoidRootPart
   end
  lp.Character:SetPrimaryPartCFrame(game:GetService("Workspace").TownLocations[where].CFrame)
end)

main:Button("Godmode",function()
  if(lp.Character:FindFirstChild("BodyHitbox")) then
    game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage", {
      Text = "Godmode does NOT work together with collecting any bubbles!"; --The chat message
      Color = Color3.fromRGB(255, 0, 0); --Chat message color, defaults to white
      Font = Enum.Font.SourceSansBold; --Chat message font, defaults to SourceSansBold
      TextSize = 24 --Text size, defaults to 18
    });
    lp.Character.BodyHitbox:Destroy()
  end
end)

main:Box("WalkSpeed","int",function(ws)
  lp.Character.Humanoid.WalkSpeed = ws
end)
