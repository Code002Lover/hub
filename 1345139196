
_G.Credits = {
  "Created by Code002Lover",
  ["Discord"] = "8ytFhuj"
}
local marketplaceService = game:GetService("MarketplaceService")
function getname(id)
  return marketplaceService:GetProductInfo(id, Enum.InfoType.Asset).Name
end
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Mystikfluu/uilib/master/uilib.lua"))()
local window = library:CreateWindow(getname(game.PlaceId))
local main = window:CreateFolder("Main")


local Sell = {}
local Names = {}
for i, v in pairs(game.Workspace:GetChildren()) do
  if(v.Name == "SellHut") then
    Sell[v.ShopBase.Island.Value] = v
    print(v.ShopBase.Island.Value)
    Names[#Names + 1] = v.ShopBase.Island.Value
  end
end

main:Dropdown("Sells", Names, true, function(value)
  _G.Sell = Sell[value].ShopBase
end)
main:Toggle("AutoSell", function(toggle)
  _G.AS = toggle
  spawn(function()
    while wait() and _G.AS do
    if(_G.Sell) then
      local CFR = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
      if(game.Players.LocalPlayer.PlayerGui.Gui.Popups.BackpackFull.Visible) then
        repeat
          game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = _G.Sell.CFrame
          wait()
        until not(game.Players.LocalPlayer.PlayerGui.Gui.Popups.BackpackFull.Visible)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFR
      end
    end
  end
end)
end)
main:Toggle("Auto Rebirth", function(toggle)
_G.AR = toggle
spawn(function()
  while _G.AR do
  game:GetService("ReplicatedStorage").Events.Rebirth:FireServer()
  wait(3)
end
end)
end)
