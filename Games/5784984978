repeat wait() until game
repeat wait() until game:GetService("VirtualUser")
repeat wait() until game.Players.LocalPlayer

local VirtualUser=game:service'VirtualUser'
game:service('Players').LocalPlayer.Idled:connect(function()
  VirtualUser:CaptureController()
  VirtualUser:ClickButton2(Vector2.new())
end)





_G.Credits = {
  "Created by Code002Lover & Ente",
  ["Discord"] = "8ytFhuj"
}
local marketplaceService = game:GetService("MarketplaceService")
local lp = game.Players.LocalPlayer
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Mystikfluu/uilib/master/uilib.lua"))()
local window = library:CreateWindow("ZeroTwo Hub")
local main = window:CreateFolder("Main")



local Pickups = game:GetService("Workspace").Pickups

function sell()
  game:GetService("ReplicatedStorage").Knit.Services.SellService.RE.Sell:FireServer()
end
function drink()
  game:GetService("ReplicatedStorage").Knit.Services.MilkService.RE.Drink:FireServer()
end
function pickup()
  for i,v in pairs(Pickups:GetChildren()) do
    v.HumanoidRootPart.CFrame = lp.Character.HumanoidRootPart.CFrame
  end
end
main:Toggle("Auto pickup gems",function(toggle)
_G.AP = toggle
while wait() and _G.AP do
pickup()
end
end)
main:Toggle("Sell",function(toggle)
_G.Sell = toggle
while wait() and _G.Sell do
sell()
end
end)
main:Toggle("Drink",function(toggle)
_G.Drink = toggle
while wait() and _G.Drink do
drink()
end
end)
