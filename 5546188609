repeat wait() until game
repeat wait() until game:GetService("VirtualUser")
repeat wait() until game.Players.LocalPlayer



local VirtualUser=game:service'VirtualUser'
game:service('Players').LocalPlayer.Idled:connect(function()
VirtualUser:CaptureController()
VirtualUser:ClickButton2(Vector2.new())
end)

_G.Credits = {
  "Created by Code002Lover & Ente_sama",
  ["Discord"] = "8ytFhuj"
}
local marketplaceService = game:GetService("MarketplaceService")
local lp = game.Players.LocalPlayer
function getname(id)
  return marketplaceService:GetProductInfo(id, Enum.InfoType.Asset).Name
end
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Mystikfluu/uilib/master/uilib.lua"))()
local window = library:CreateWindow(getname(game.PlaceId))
local main = window:CreateFolder("Main")
local misc = window:CreateFolder("Misc")


main:Toggle("Auto Click",function(toggle)
  _G.AC = toggle
  while wait() and _G.AC do
    local args = {
        [1] = "DefaultScythe"
    }

    game:GetService("ReplicatedStorage").ScytheEvent:FireServer(unpack(args))
  end
end)

main:Toggle("Auto kill npcs",function(toggle)
  _G.AKN = toggle
  while wait() and _G.AKN do
    for i,v in pairs(workspace.EnemyContainer:GetChildren()) do
      if(v:FindFirstChild("Humanoid")) then
        if(v.Humanoid.Health ~= 0) then
          repeat
            game:GetService("ReplicatedStorage").DamageEvent:FireServer(v.Humanoid)
            wait()
          until v:FindFirstChild("Humanoid") == nil or v.Parent == nil or v.Humanoid.Health == 0
        end
      end
    end
  end
end)

codes = {
  "followers",
  "subtocookie",
  "FREE PET",
  "Tofuu",
  "cool",
  "RussoPlays"
}

misc:Button("Use Codes",function()
  for i,v in pairs(codes) do
    local args = {
        [1] = v
    }

    game:GetService("ReplicatedStorage").CodeCheck:InvokeServer(unpack(args))
  end
end)

main:Toggle("Auto Collect EXP Stars",function(toggle)
  _G.ACES = toggle
  while wait() and _G.ACES do
    for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
      if(string.find(v.Name,"Star") and v:IsA("UnionOperation")) then
        v.CFrame = lp.Character.HumanoidRootPart.CFrame
      end
    end
  end
end)
